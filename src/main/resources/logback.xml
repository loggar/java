<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%p]\t[%thread]\t%d{dd/MM/YYYY HH:mm:ss.SSS} %C.%method - %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="com.loggar">
        <level value="debug" />
    </logger>
    <root>
        <level value="debug" />
        <appender-ref ref="console" />
    </root>
</configuration>
